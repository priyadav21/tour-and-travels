{
  "homepage": "https://priyadav21.github.io/tour-and-travels",
  "name": "Touroxy",
  "version": "1.0.0",
  "description": "Touroxy",
  "engines": {
    "node": "10.14.2"
  },
  "proxy": "https://api.touroxy.com",
  "main": "index.js",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "node build/bundle.js",
    "build:prod": "webpack  --config webpack.server.js && webpack -p --config webpack.client.prod.js --mode production",
    "prod": "npm install --force && npm run build:prod",
    "webpack-prod": "webpack -p --progress --config webpack.client.prod.js --mode production",
    "dev": "npm run build-server && npm-run-all --parallel dev:*",
    "build-server": "webpack --config webpack.server.js",
    "dev:build-server": "webpack --config webpack.server.js --watch",
    "dev:build-client": "webpack --config webpack.client.js --watch",
    "dev:server": "nodemon --watch build --exec \"node build/bundle.js\"",
    "precommit": "pretty-quick --staged"
  },
  "author": "spysr",
  "license": "private",
